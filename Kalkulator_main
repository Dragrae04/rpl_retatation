    private void innamaActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
         String inputNama1 = innama.getText();
        inputNama = inputNama1;

        shnama.setText(inputNama);
    }                                      

    private void innpwpActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
        String inputNPWP1 = innpwp.getText();
        inputNPWP = inputNPWP1;

        shnpwp.setText(inputNPWP);
    }                                      

    private void inpendapatanActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
        String inputPendapatan = inpendapatan.getText();
        gajiin = Integer.parseInt(inputPendapatan);

        while(gajiin<0){
            shpendapatan.setText("ERROR!!!");
        }

        double gajirp = gajiin;
        double gajisthn = gajirp*12;

        DecimalFormat df = (DecimalFormat) DecimalFormat.getCurrencyInstance();
        DecimalFormatSymbols dfs = new DecimalFormatSymbols();
        dfs.setCurrencySymbol("");
        dfs.setMonetaryDecimalSeparator(',');
        dfs.setGroupingSeparator('.');df.setDecimalFormatSymbols(dfs);

        String gajirp1 = "Rp. " + df.format(gajirp);
        shpendapatan.setText(gajirp1);

        String gajirpsthn = "Rp. " + df.format(gajisthn);
        ingajisetahun.setText(gajirpsthn);
        shgajisetahun.setText(gajirpsthn);
        // ingajisetahun.setText();
    }                                            

    private void ingajisetahunActionPerformed(java.awt.event.ActionEvent evt) {                                              
        // TODO add your handling code here:
    }                                             

    private void inbuttonMouseClicked(java.awt.event.MouseEvent evt) {                                      
        // TODO add your handling code here:
        shnama.setText(inputNama);
        shnpwp.setText(inputNPWP);
        shpendapatan.setText("Rp" + Integer.toString(gajiin));
    }                                     

